    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.4.0",
    "apollo-boost": "^0.4.9",
    "bootstrap": "^4.5.0",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.4.1",
    "firebase": "^7.15.3",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-firebase": "^0.2.0-beta.4",
    "gatsby-plugin-manifest": "^2.2.38",
