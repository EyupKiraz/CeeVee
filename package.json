    "graphql": "^15.2.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.1.1",
    "react-dom": "^16.12.0",
    "react-firebase-hooks": "^2.2.0",
    "react-fontawesome": "^1.7.1",
    "react-github-btn": "^1.1.1",
    "react-helmet": "^5.2.1",
    "react-progress-button": "^5.1.0",
    "react-redux": "^7.2.0",
    "react-reveal": "^1.2.2",
    "react-tilt": "^0.1.4",
